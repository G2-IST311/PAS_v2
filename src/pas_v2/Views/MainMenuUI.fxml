<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="900.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="pas_v2.Controllers.MainMenuController">
   <children>
      <MenuBar fx:id="menuBar" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <menus>
          <Menu mnemonicParsing="false" text="PAS">
            <items>
              <MenuItem fx:id="logoutButton" mnemonicParsing="false" onAction="#logoutBtnClicked" text="Log Out" />
                  <MenuItem mnemonicParsing="false" text="Quit" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <TabPane layoutX="8.0" layoutY="29.0" prefHeight="571.0" prefWidth="900.0" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="29.0">
        <tabs>
          <Tab text="Find Swimmer">
            <fx:include fx:id="findSwimmerTabPage" source="FindSwimmerUI.fxml"/>
          </Tab>
          <Tab text="View Pool">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
          </Tab>
            <Tab text="Reports">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
              </content>
            </Tab>
            <Tab text="Staff">
               <fx:include fx:id="staffTabPage" source="StaffUI.fxml"/>
            </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
